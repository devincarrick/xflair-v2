# Example environment variables
# Copy this file to .env.local and fill in your actual values

# AWS Configuration
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_S3_BUCKET_NAME=your-s3-bucket-name

# Cloudinary Configuration
fGifURL=cloudinary://<your_api_key>:<your_api_secret>@cloudinary-cloud-name
hGifURL=cloudinary://<your_api_key>:<your_api_secret>@cloudinary-cloud-name

# Service Host (optional, defaults to localhost)
SERVICE_HOST=localhost

# Mode (optional, for logs)
mode=logs